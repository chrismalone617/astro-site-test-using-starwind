---
import Layout from "@/layouts/Layout.astro";
import Navigation from "@/components/Navigation.astro";
import Hero from "@/components/Hero.astro";
import InfoBoxRow from "@/components/InfoBoxRow.astro";
import TabsSection from "@/components/TabsSection.astro";
import FAQSection from "@/components/FAQSection.astro";
import ButtonGroupSection from "@/components/ButtonGroupSection.astro";
import Footer from "@/components/Footer.astro";
---

<Layout title="Astro + Starwind UI Landing">
  <Navigation links={[
    { href: "/components", label: "Components" },
    { href: "#features", label: "Features" },
    { href: "#faq", label: "FAQ" },
    { href: "#contact", label: "Contact" },
  ]} />
  <Hero />
  <InfoBoxRow />
  <TabsSection />
  <FAQSection />
  <ButtonGroupSection />
  <Footer links={[
  { href: "/components", label: "Components" },
  { href: "#features", label: "Features" },
  { href: "#faq", label: "FAQ" },
  { href: "#contact", label: "Contact" },
  { href: "/privacy", label: "Privacy" },
  { href: "/terms", label: "Terms" },
]} />
</Layout>
